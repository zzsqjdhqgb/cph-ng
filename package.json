            "type": "string",
            "default": "-cp .",
            "markdownDescription": "%cph-ng.configuration.compilation.properties.javaArgs.markdownDescription%"
          },
          "cph-ng.compilation.javaRunner": {
            "type": "string",
            "default": "java",
            "markdownDescription": "%cph-ng.configuration.compilation.properties.javaRunner.markdownDescription%"
          },
          "cph-ng.compilation.javaRunArgs": {
            "type": "string",
            "default": "",
            "markdownDescription": "%cph-ng.configuration.compilation.properties.javaRunArgs.markdownDescription%"
          },
          "cph-ng.compilation.pythonCompiler": {
            "type": "string",
            "default": "pypy3",
            "markdownDescription": "%cph-ng.configuration.compilation.properties.pythonCompiler.markdownDescription%"
          },
          "cph-ng.compilation.pythonArgs": {
            "type": "string",
            "default": "",
            "markdownDescription": "%cph-ng.configuration.compilation.properties.pythonArgs.markdownDescription%"
          },
          "cph-ng.compilation.pythonRunner": {
            "type": "string",
            "default": "pypy3",
            "markdownDescription": "%cph-ng.configuration.compilation.properties.pythonRunner.markdownDescription%"
          },
          "cph-ng.compilation.pythonRunArgs": {
            "type": "string",
            "default": "",
            "markdownDescription": "%cph-ng.configuration.compilation.properties.pythonRunArgs.markdownDescription%"
          },
          "cph-ng.compilation.javascriptRunner": {
            "type": "string",
            "default": "node",
            "markdownDescription": "%cph-ng.configuration.compilation.properties.javascriptRunner.markdownDescription%"
          },
          "cph-ng.compilation.javascriptRunArgs": {
            "type": "string",
            "default": "",
            "markdownDescription": "%cph-ng.configuration.compilation.properties.javascriptRunArgs.markdownDescription%"
          },
          "cph-ng.compilation.objcopy": {
            "type": "string",
            "default": "objcopy",
            "markdownDescription": "%cph-ng.configuration.compilation.properties.objcopy.markdownDescription%"
          },
          "cph-ng.compilation.timeout": {
            "type": "number",
