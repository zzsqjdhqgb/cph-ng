      {
        "id": "bfCompare",
        "order": 4,
        "title": "%cph-ng.configuration.bfCompare.title%",
        "properties": {
          "cph-ng.bfCompare.generatorTimeLimit": {
            "type": "number",
            "default": 10000,
            "markdownDescription": "%cph-ng.configuration.bfCompare.properties.generatorTimeLimit.markdownDescription%"
