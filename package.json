              "%cph-ng.configuration.problem.properties.expandBehavior.enumDescriptions.firstFailed%",
              "%cph-ng.configuration.problem.properties.expandBehavior.enumDescriptions.same%"
            ]
          },
          "cph-ng.problem.templateFile": {
            "type": "string",
            "default": "",
            "markdownDescription": "%cph-ng.configuration.problem.properties.templateFile.markdownDescription%"
          },
          "cph-ng.problem.problemFilePath": {
            "type": "string",
            "default": "${workspace}/.cph-ng/${relativeDirname}/${basename}.bin",
            "markdownDescription": "%cph-ng.configuration.problem.properties.problemFilePath.markdownDescription%"
          },
          "cph-ng.problem.unzipFolder": {
            "type": "string",
            "default": "${workspace}/.cph-ng/${zipBasenameNoExt}",
